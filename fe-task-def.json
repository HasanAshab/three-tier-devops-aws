{
    "family": "frontend-task-def",
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "ghcr.io/hasanashab/spring-react-devops-appservice-frontend:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "frontend-4200-tcp",
                    "containerPort": 4200,
                    "hostPort": 4200,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "systemControls": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::958948421283:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "ephemeralStorage": {
        "sizeInGiB": 21
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "enableFaultInjection": false
}